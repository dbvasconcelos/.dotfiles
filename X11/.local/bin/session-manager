#!/bin/sh

# Displays the session options in dmenu for choosing which to run

# First field is how the option appears on the menu
# and the second is the command to be run
readonly options="\
 lock: slock
 logoff: bspc quit
 renew wm: bspc wm -r
 reboot: reboot
 shutdown: shutdown now"

run-command-prompt "$options" "Session"
